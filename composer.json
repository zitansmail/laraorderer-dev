{
  "name": "zitansmail/laravel-migration-orderer",
  "description": "Automatically order Laravel migrations based on foreign key dependencies to prevent constraint errors",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "migrations",
    "database",
    "dependencies",
    "foreign-keys",
    "topological-sort"
  ],
  "homepage": "https://github.com/zitansmail/laravel-migration-orderer",
  "support": {
    "issues": "https://github.com/zitansmail/laravel-migration-orderer/issues",
    "source": "https://github.com/zitansmail/laravel-migration-orderer"
  },
  "authors": [
    {
      "name": "zitansmail",
      "email": "zitansmail@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MigrationOrderer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0 || ^11.0 || ^12.0",
    "illuminate/console": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "orchestra/testbench": "^10.6 || ^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MigrationOrderer\\MigrationOrdererServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  }
}
